.overlay {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  height: 100%;
  width: 100%;
  opacity: 0;
  transition: .5s ease;
  background: #050505;
  border-radius: 20px;
  background: -webkit-linear-gradient(to top, rgba(5, 5, 5, 0.5), rgba(216, 202, 202, 0.5));
  background: linear-gradient(to top, rgba(5, 5, 5, 0.5), rgba(216, 202, 202, 0.5));
  text-decoration: underline;
}

.container:hover .overlay {
  opacity: 1;
}

.text {
  color: white;
  font-size: 20px;
  text-decoration: underline;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  -ms-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  text-align: center;
}

.heading{
	margin: 10px;
	text-align:center;
